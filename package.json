{
  "name": "arctic-viewer",
  "description": "Standalone application useful to load and explore InSitu/Batch data products.",
  "license": "BSD-3-Clause",
  "version": "0.0.0-semantically-release",
  "main": "./lib/arctic-viewer.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kitware/arctic-viewer.git"
  },
  "dependencies": {
    "tonic-data-manager": "0.0.11",
    "tonic-image-builder": "0.0.8",
    "tonic-mouse-handler": "0.0.6",
    "tonic-query-data-model": "0.0.13",
    "tonic-widgets": "0.0.9",

    "body-parser": "^1.14.0",
    "commander": "latest",
    "connect-gzip-static": "latest",
    "cz-conventional-changelog": "1.1.0",
    "du": "0.1.0",
    "express": "latest",
    "font-awesome": "4.4.0",
    "font-awesome-webpack": "0.0.3",
    "gl-matrix": "2.3.1",
    "http-proxy": "latest",
    "mkdirp": "^0.5.1",
    "monologue.js": "0.3.3",
    "mout": "0.11.0",
    "normalize.css": "3.0.3",
    "open": "latest",
    "progress": "latest",
    "react": "0.13.3",
    "tarball-extract": "latest",

    "less": "^1.5"
  },
  "devDependencies": {
  },
  "scripts": {
    "commit": "git-cz",
    "build": "webpack",
    "build:debug": "webpack --display-modules",
    "build:release": "webpack -p",
    "www": "tonic-site-generator site.config.js",
    "www:http": "tonic-site-generator site.config.js --local-test && serve docs/www",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "bin": {
    "ArcticViewer": "./bin/arctic-viewer-cli.js"
  },
  "preferGlobal": true,
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
